{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        [
          "@babel/plugin-syntax-jsx"
        ],
        [
          "@babel/plugin-transform-runtime"
        ],
        ["@babel/plugin-syntax-class-properties"],
        [
          "@babel/plugin-proposal-class-properties",
          {
            "loose": true
          }
        ]
    ]
  }